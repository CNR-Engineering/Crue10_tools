<?xml version="1.0" encoding="UTF-8"?>
<DFRT xmlns="http://www.fudaa.fr/xsd/crue" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.fudaa.fr/xsd/crue http://www.fudaa.fr/xsd/crue/dfrt-1.2.xsd">
  <Commentaire></Commentaire>
  <LoiFFs>
    {%- for friction_law in friction_law_list %}
    <LoiFF Nom="{{ friction_law.id }}" Type="{{ friction_law.type }}">
      <Commentaire></Commentaire>
      <EvolutionFF>
        {% for z, k in friction_law.loi_Fk -%}
        <PointFF>{{ z }} {{ k }}</PointFF>
        {%- endfor %}
      </EvolutionFF>
    </LoiFF>
    {%- endfor %}
  </LoiFFs>
</DFRT>
